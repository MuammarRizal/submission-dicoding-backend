{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": "off",
    "no-console": "off",
    "quotes": "off",
    "no-unused-vars": "off",
    "arrow-body-style": "off",
    "prefer-arrow-callback": "warn",
    "no-unused-expressions": "off",
    "eslint-plugin-import/export": "disable"
  }
}
